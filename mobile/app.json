{
  "expo": {
    "name": "ScannerMobile",
    "slug": "scanner-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "jsEngine": "hermes",
    "experiments": {
      "typedRoutes": false
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {},
    "plugins": [
      "expo-secure-store"
    ]
  }
}
